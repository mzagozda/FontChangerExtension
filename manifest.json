{
  "background": {
    "service_worker": "service_worker.js"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "js/cs.js",
        "js/default.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "description": "Change the font on Facebook, Twitter, Google, Youtube or any other site. Test Google Web Fonts™.",
  "icons": {
    "128": "img/logo.png",
    "48": "img/logo48.png"
  },
  "key": "MIGfMA0GCSqGSIb3DQEBAQUAA4GNADCBiQKBgQC5ZYbsr6ep/2ofkCblxZX7M4WhZQ1daYrmHSoiEbvpFvnuqgVBGk3yqwF0xCrqy6kueUAHRY696qJGSKW5Uxudj3e4CmVwZMbQivP7VzpoUlzLTBaKkZgavGuR9GSAzkUvY/xl8JHzt7dIKIq3CUEMGpDAoBfzX4o4a37JA+hfPQIDAQAB",
  "manifest_version": 3,
  "name": "Font Changer with Google Web Fonts™",
  "permissions": [
    "storage",
    "unlimitedStorage",
    "notifications",
    "declarativeNetRequest",
    "tabs",
    "scripting",
    "activeTab"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "3.1.0",
  "web_accessible_resources": [
    {
      "resources": [
        "js/custom_fonts.js",
        "img/logo48.png",
        "js/cs.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "action": {
    "default_icon": "img/logo.png",
    "default_popup": "popup.html",
    "default_title": "Font Changer"
  }
}
